datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.myprisma/client"
}

model Item {
  id                 String    @id @default(cuid())
  createdAt          DateTime  @default(now())
  updatedAt          DateTime  @default(now())
  label              String    @default("")
  complete           Boolean   @default(false)
  list               List?     @relation("Item_list", fields: [listId], references: [id])
  listId             String?   @map("list")
  project            Project?  @relation("Item_project", fields: [projectId], references: [id])
  projectId          String?   @map("project")
  tags               Tag[]     @relation("Item_tags")
  from_List_items    List[]    @relation("List_items")
  from_Project_items Project[] @relation("Project_items")
  from_Tag_items     Tag[]     @relation("Tag_items")

  @@index([listId])
  @@index([projectId])
}

model List {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @default(now())
  label          String   @default("")
  items          Item[]   @relation("List_items")
  from_Item_list Item[]   @relation("Item_list")
}

model Project {
  id                String   @id @default(cuid())
  createdAt         DateTime @default(now())
  updatedAt         DateTime @default(now())
  label             String   @default("")
  items             Item[]   @relation("Project_items")
  from_Item_project Item[]   @relation("Item_project")
}

model Tag {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @default(now())
  label          String   @default("")
  items          Item[]   @relation("Tag_items")
  from_Item_tags Item[]   @relation("Item_tags")
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  about     String    @default("")
  createdAt DateTime? @default(now())
}
